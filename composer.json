{
    "name": "gssrinivas59/slim3-api-skeleton",
    "description": "Slim 3 API Skeleton with content negotiation and performance in mind.",
    "type": "project",
    "keywords": ["Slim-framework", "API", "Skeleton"],
    "homepage": "https://github.com/gssrinivas59/slim3-api-skeleton",
    "license": "MIT",
    "authors": [
        {
            "name": "G Sudhir Srinivas",
            "email": "gssrinivas05@gmail.com"
        }
    ],
    "require": {
        "slim/slim": "^3.5",
        "slim/http-cache": "^0.3.0",
        "monolog/monolog": "^1.21",
        "hassankhan/config": "^0.10.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.5 < 6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"file_exists('env.php.dist') || copy('env.php.dist', 'env.php');\""
        ]
    },
    "config": {
        "preferred-install": "dist"
    }
}
